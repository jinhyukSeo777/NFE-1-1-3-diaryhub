{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app", "plugin:prettier/recommended"],
  "plugins": ["react", "prettier"],
  "rules": {
    "react/prop-types": "off",
    "no-console": "warn",
    "prettier/prettier": ["error", { "endOfLine": "crlf" }]
  }
}
